function clicklink(e) {
  loading();
  //console.log(e.getAttribute("href"));
  ipcRenderer.send('element-clicked', e.getAttribute("href"));
  e.preventDefault();
  return false;
}

function clickchan(e, link) {
  loading();
  //console.log(link);
  ipcRenderer.send('chan-clicked', link);
  e.preventDefault();
}

function searchResults(e) {
  loading();
  var form = document.forms[0];
  var name = form.elements["search_query"].value;
  console.log("search: " + name);
  ipcRenderer.send('search', name);
  return false;
}

function loading() {
  var iframe = document.getElementById("loadinggif");
  iframe.style.zIndex = 99999;
}

