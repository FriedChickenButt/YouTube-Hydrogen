{
  "name": "YouTube-Hydrogen",
  "productName": "YouTube-Hydrogen",
  "version": "1.0.0",
  "description": "YouTube-Hydrogen Desktop App",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Youtube-Hydrogen\""
  },
  "author": "Swao",
  "license": "MIT",
  "devDependencies": {
    "electron-dl": "^1.12.0",
    "electron-packager": "^12.1.1"
  }
}
